PATH=$HOME/.cask/bin:$PATH
PATH=$HOME/.rbenv/bin:$PATH
PATH=$HOME/bin:$PATH
PATH=$HOME/go/bin:$PATH
PATH=$HOME/npm/bin:$PATH
PATH=$HOME/.yarn/bin:$PATH
PATH=$HOME/.nodenv/bin:$PATH
PATH=$HOME/opt/android-studio/bin:$PATH
export PATH

eval "$(rbenv init -)"
eval "$(nodenv init -)"
